@prefix : <http://www.researchspace.org/resource/system/service/custom#> .

[] a repo:Repository ;
  repo:repositoryID "geo" ;
  rdfs:label "A wrapper for the SQL test repository." ;
  repo:repositoryImpl [
    repo:repositoryType "openrdf:SailRepository" ;
    reposail:sailImpl [
      sail:sailType "researchspace:SQLSail" ;
      ephedra:serviceURL "jdbc:postgresql://vitgeo.cjaiad67yr5p.eu-central-1.rds.amazonaws.com:5432/" ;
      rsrepo:username "${archipelago.username}" ;
      rsrepo:password "${archipelago.password}" ;
      ephedra:implementsService :geosql ;
    ] ;
  ] .